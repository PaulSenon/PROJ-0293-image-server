{
  "name": "image-server",
  "version": "1.0.0",
  "description": "Environment-agnostic image resizing server",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/localEntrypoint.ts",
    "build": "rimraf dist && tsc",
    "build:lambda": "rimraf dist && node esbuild.lambda.js",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rimraf dist node_modules"
  },
  "keywords": [
    "image",
    "resize",
    "server",
    "sharp",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=22.0.0"
  },
  "packageManager": "pnpm@10.6.1+sha512.40ee09af407fa9fbb5fbfb8e1cb40fbb74c0af0c3e10e9224d7b53c7658528615b2c92450e74cfad91e3a2dcafe3ce4050d80bda71d757756d2ce2b66213e9a3",
  "devDependencies": {
    "@tsconfig/node22": "^22.0.0",
    "@types/aws-lambda": "^8.10.147",
    "@types/node": "^22.13.9",
    "esbuild": "^0.25.0",
    "rimraf": "^6.0.1",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "vitest": "^3.0.7"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.758.0",
    "@types/express": "^5.0.0",
    "exif-reader": "^2.0.2",
    "express": "^4.21.2",
    "iptc-reader": "^1.0.1",
    "lambda-stream": "^0.5.0",
    "sharp": "^0.33.5",
    "zod": "^3.24.2"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp"
